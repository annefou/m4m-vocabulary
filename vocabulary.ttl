gdmt:hasDistributionQueryStatement a owl:DatatypeProperty;
  rdfs:label "has Distribution Query Statement"@en;
  skos:definition "Technical statement(s) for querying dataset distribution."@en;
  skos:prefLabel "has Distribution Query Statement"@en .

gdmt:hasDistributionDate a owl:DatatypeProperty;
  rdfs:label "has Distribution Date"@en;
  skos:definition "Date when the dataset was distributed."@en;
  skos:prefLabel "has Distribution Date"@en .

gdmt:hasDistributorIdentifier a owl:DatatypeProperty;
