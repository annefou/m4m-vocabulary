gdmt:hasLicenseIdentifierSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has License Identifier Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Rights Identifier."@en;
  skos:prefLabel "has License Identifier Scheme IRI"@en .

gdmt:hasDatasetDate a owl:DatatypeProperty;
  rdfs:label "has Dataset Date"@en;
  skos:definition "Date relevant to dataset."@en;
  skos:prefLabel "has Dataset Date"@en .

gdmt:hasDatasetDateType a owl:ObjectProperty;
  rdfs:label "has Dataset Date Type"@en;
  skos:definition "Type of the date with respect to the dataset."@en;
  skos:prefLabel "has Dataset Date Type"@en .

gdmt:hasDistributionIdentifier a owl:DatatypeProperty;
  rdfs:label "has Distribution Identifier"@en;
  skos:definition "A globally unique string that identifies the dataset distribution."@en;
  skos:prefLabel "has Distribution Identifier"@en .

gdmt:hasDistributionIdentifierType a owl:ObjectProperty;
  rdfs:label "has Distribution Identifier Type"@en;
  skos:definition "The identifier type used to identify the dataset distribution."@en;
  skos:prefLabel "has Distribution Identifier Type"@en .

gdmt:hasDistributionMediaType a owl:ObjectProperty;
  rdfs:label "has Distribution Media Type"@en;
  skos:definition "A media type, formerly known as a MIME type, of dataset distribution."@en;
  skos:prefLabel "has Distribution Media Type"@en .

gdmt:hasDistributionSize a owl:DatatypeProperty;
  rdfs:label "has Distribution Size"@en;
  skos:definition "Total size of dataset distribution."@en;
  skos:prefLabel "has Distribution Size"@en .

gdmt:hasDistributionAccessProtocol a owl:DatatypeProperty;
  rdfs:label "has Distribution Access Protocol"@en;
  skos:definition "The protocol used to access the dataset distribution."@en;
