  skos:definition "Globally unique string that identifies the distributor (an individual or legal entity)."@en;
  skos:prefLabel "has Distributor Identifier"@en .

gdmt:hasDistributorIdentifierScheme a owl:DatatypeProperty;
  rdfs:label "has Distributor Identifier Scheme"@en;
