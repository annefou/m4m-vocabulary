gdmt:hasResourceTypeInfo a owl:ObjectProperty;
  rdfs:label "has Resource Type Info"@en;
  skos:definition "Information about the type of the resource being described with metadata."@en;
  skos:prefLabel "has Resource Type Info"@en .

gdmt:hasDatasetIdentifierInfo a owl:ObjectProperty;
  rdfs:label "has Dataset Identifier Info"@en;
  skos:definition "Information about the globally unique and persistent identifier used to identify and optionally access (meta)data of the dataset being described."@en;
  skos:prefLabel "has Dataset Identifier Info"@en .

gdmt:hasRelatedResourcesInfo a owl:ObjectProperty;
  rdfs:label "has Related Resources Info"@en;
  skos:definition "Information about resource related to the dataset or other entity being described."@en;
  skos:prefLabel "has Related Resources Info"@en .

gdmt:hasVersionInfo a owl:ObjectProperty;
  rdfs:label "has Version Info"@en;
  skos:definition "Information about the version of the resource being described."@en;
  skos:prefLabel "has Version Info"@en .

gdmt:hasLanguageInfo a owl:ObjectProperty;
  rdfs:label "has Language Info"@en;
  skos:definition "Information about the language in which the dataset being described is provided."@en;
  skos:prefLabel "has Language Info"@en .

gdmt:hasTitleInfo a owl:ObjectProperty;
  rdfs:label "has Title Info"@en;
  skos:definition "Information about a name or title by which the dataset being described is known."@en;
  skos:prefLabel "has Title Info"@en .

gdmt:hasDescriptionInfo a owl:ObjectProperty;
  rdfs:label "has Description Info"@en;
  skos:definition "Detailed information (typically at the level of an abstract) about the dataset being described."@en;
  skos:prefLabel "has Description Info"@en .

gdmt:hasSubjectInfo a owl:ObjectProperty;
  rdfs:label "has Subject Info"@en;
  skos:definition "Topic information that defines the dataset's subject or purpose."@en;
  skos:prefLabel "has Subject Info"@en .

gdmt:hasSubjectAndKeywordInfo a owl:ObjectProperty;
  rdfs:label "has Subject And Keyword Info"@en;
  skos:definition "Concepts (keywords, classification, or free text terms) that define the dataset or purpose (subjects which can be addressed) using the dataset."@en;
  skos:prefLabel "has Subject And Keyword Info"@en .

gdmt:hasControlledTermsInfo a owl:ObjectProperty;
  rdfs:label "has Controlled Terms Info"@en;
  skos:definition "Controlled concepts that define the dataset or purpose."@en;
  skos:prefLabel "has Controlled Terms Info"@en .

gdmt:hasCreatorInfo a owl:ObjectProperty;
  rdfs:label "has Creator Info"@en;
  skos:definition "Information about the Creator entity that brought into existence the dataset being described. Creators can be people, organizations and/or physical or virtual infrastructure (e.g., sensors, software)."@en;
  skos:prefLabel "has Creator Info"@en .

gdmt:hasCreatorNameInfo a owl:ObjectProperty;
  rdfs:label "has Creator Name Info"@en;
  skos:definition "Information about the creator name."@en;
  skos:prefLabel "has Creator Name Info"@en .

gdmt:hasCreatorIdentifierInfo a owl:ObjectProperty;
  rdfs:label "has Creator Identifier Info"@en;
  skos:definition "Information about the creator identifier."@en;
  skos:prefLabel "has Creator Identifier Info"@en .

gdmt:hasCreatorAffiliationInfo a owl:ObjectProperty;
  rdfs:label "has Creator Affiliation Info"@en;
  skos:definition "Information about the creator affiliations."@en;
  skos:prefLabel "has Creator Affiliation Info"@en .

gdmt:hasContributorInfo a owl:ObjectProperty;
  rdfs:label "has Contributor Info"@en;
  skos:definition "Information about a Contributor entity that helped bring into existence the dataset being described. Contributors can be people, organizations and/or physical or virtual infrastructure (e.g., sensors, software)."@en;
  skos:prefLabel "has Contributor Info"@en .

gdmt:hasContributorNameInfo a owl:ObjectProperty;
  rdfs:label "has Contributor Name Info"@en;
  skos:definition "Information about a contributor name."@en;
  skos:prefLabel "has Contributor Name Info"@en .

gdmt:hasContributorIdentifierInfo a owl:ObjectProperty;
  rdfs:label "has Contributor Identifier Info"@en;
  skos:definition "Information about a contributor identifier."@en;
  skos:prefLabel "has Contributor Identifier Info"@en .

gdmt:hasContributorAffiliationInfo a owl:ObjectProperty;
  rdfs:label "has Contributor Affiliation Info"@en;
  skos:definition "Information about a contributor's affiliations."@en;
  skos:prefLabel "has Contributor Affiliation Info"@en .

gdmt:hasPublisherInfo a owl:ObjectProperty;
  rdfs:label "has Publisher Info"@en;
  skos:definition "Information about the entity that plays the principal role in officially making the dataset public. The role can be to hold, archive, publish, print, distribute, release, issue, or produce the dataset being described. This property is often used to formulate the citation, so consider the prominence of the role and of the entity. For datasets, use the primary repository for serving the data, or the primary organization taking responsibility for serving the data, as the Publisher. The Distribution element can be used to describe the entity(ies) that make the dataset accessible."@en;
  skos:prefLabel "has Publisher Info"@en .

gdmt:hasPublisherIdentifierInfo a owl:ObjectProperty;
  rdfs:label "has Publisher Identifier Info"@en;
  skos:definition "Information about the publisher identifier."@en;
  skos:prefLabel "has Publisher Identifier Info"@en .

gdmt:hasRightsInfo a owl:ObjectProperty;
  rdfs:label "has Rights Info"@en;
  skos:definition "Information about ways in which the dataset may or may not be accessed and used."@en;
  skos:prefLabel "has Rights Info"@en .

gdmt:hasLicenseIdentifierInfo a owl:ObjectProperty;
  rdfs:label "has License Identifier Info"@en;
  skos:definition "Information about the license identifier."@en;
  skos:prefLabel "has License Identifier Info"@en .

gdmt:hasDateInfo a owl:ObjectProperty;
  rdfs:label "has Date Info"@en;
  skos:definition "Information about relevant dates related to the resource being described. One instance of date must be of datsetDateType=Published."@en;
  skos:prefLabel "has Date Info"@en .

gdmt:hasDistributionInfo a owl:ObjectProperty;
  rdfs:label "has Distribution Info"@en;
  skos:definition "Details about the distribution for this individual public presentation of the dataset (if part of the Distribution metadata), or for all the public presentations of the dataset being described (if part of the Dataset metadata)."@en;
  skos:prefLabel "has Distribution Info"@en .

gdmt:hasDistributionAccessInfo a owl:ObjectProperty;
  rdfs:label "has Distribution Access Info"@en;
  skos:definition "Details about the distribution access."@en;
  skos:prefLabel "has Distribution Access Info"@en .

gdmt:hasDistributorInfo a owl:ObjectProperty;
  rdfs:label "has Distributor Info"@en;
  skos:definition "Information about the distributor of this dataset distribution. The Distributor includes persons or legal entities that responsible for distributing the dataset distribution."@en;
  skos:prefLabel "has Distributor Info"@en .

gdmt:hasDistributorNameInfo a owl:ObjectProperty;
  rdfs:label "has Distributor Name Info"@en;
  skos:definition "Information about the distributor name."@en;
  skos:prefLabel "has Distributor Name Info"@en .

gdmt:hasDistributorIdentifierInfo a owl:ObjectProperty;
  rdfs:label "has Distributor Identifier Info"@en;
  skos:definition "Information about the distributor identifier."@en;
  skos:prefLabel "has Distributor Identifier Info"@en .

gdmt:hasDistributorAffiliationInfo a owl:ObjectProperty;
  rdfs:label "has Distributor Affiliation Info"@en;
  skos:definition "Information about the distributor affiliations."@en;
  skos:prefLabel "has Distributor Affiliation Info"@en .

gdmt:hasContentInfo a owl:ObjectProperty;
  rdfs:label "has Content Info"@en;
  skos:definition "Information describing the entities that constitute the dataset being described, and their organization and provenance. (The dataset is defined as coming from a single Data Source.)"@en;
  skos:prefLabel "has Content Info"@en .

gdmt:hasDataStreamInfo a owl:ObjectProperty;
  rdfs:label "has Data Stream Info"@en;
  skos:definition "Information about a Data Stream, a collection of one or more variable records originating from a single data source (e.g., sensor, process, or person). A data stream can grow over time."@en;
  skos:prefLabel "has Data Stream Info"@en .

gdmt:hasDataSourceInfo a owl:ObjectProperty;
  rdfs:label "has Data Source Info"@en;
  skos:definition "Information about a Data Source, a physical or conceptual entity that creates the data streams that make up the described dataset. The data source may or may not be associated with a fixed location; for example, a series of discrete sensors deployed over time to a single location may be considered a single data source in some systems."@en;
  skos:prefLabel "has Data Source Info"@en .

gdmt:hasVariableInfo a owl:ObjectProperty;
  rdfs:label "has Variable Info"@en;
  skos:definition "Information about a single entity being measured or computed."@en;
  skos:prefLabel "has Variable Info"@en .

gdmt:hasSpatialCoverageInfo a owl:ObjectProperty;
  rdfs:label "has Spatial Coverage Info"@en;
  skos:definition "Information about the geospatial area (region on Earth) covered by the dataset being described."@en;
  skos:prefLabel "has Spatial Coverage Info"@en .

gdmt:hasSpatialCoveragePointInfo a owl:ObjectProperty;
  rdfs:label "has Spatial Coverage Point Info"@en;
  skos:definition "Information about a geospatial coverage represented as a single point."@en;
  skos:prefLabel "has Spatial Coverage Point Info"@en .

gdmt:hasSpatialCoverageBoundingBoxInfo a owl:ObjectProperty;
  rdfs:label "has Spatial Coverage Bounding Box Info"@en;
  skos:definition "Information about a geospatial coverage represented using bounding box."@en;
  skos:prefLabel "has Spatial Coverage Bounding Box Info"@en .

gdmt:hasSpatialCoveragePolygonInfo a owl:ObjectProperty;
  rdfs:label "has Spatial Coverage Polygon Info"@en;
  skos:definition "Information about a geospatial coverage represented using polygon."@en;
  skos:prefLabel "has Spatial Coverage Polygon Info"@en .

gdmt:hasVerticalCoverageInfo a owl:ObjectProperty;
  rdfs:label "has Vertical Coverage Info"@en;
  skos:definition "Information about the vertical area (altitude and/or depth) covered by the dataset being described."@en;
  skos:prefLabel "has Vertical Coverage Info"@en .

gdmt:hasTemporalCoverageInfo a owl:ObjectProperty;
  rdfs:label "has Temporal Coverage Info"@en;
  skos:definition "Information about the temporal coverage and resolution of the dataset being described."@en;
  skos:prefLabel "has Temporal Coverage Info"@en .

gdmt:hasFundingInfo a owl:ObjectProperty;
  rdfs:label "has Funding Info"@en;
  skos:definition "Information about the sources of funding that enabled and drove the creation of the dataset. (Non-monetary sources are described under Contributor.)"@en;
  skos:prefLabel "has Funding Info"@en .

gdmt:hasAwardInfo a owl:ObjectProperty;
  rdfs:label "has Award Info"@en;
  skos:definition "Information about the project award."@en;
  skos:prefLabel "has Award Info"@en .

gdmt:hasFunderInfo a owl:ObjectProperty;
  rdfs:label "has Funder Info"@en;
  skos:definition "Information about the award funder."@en;
  skos:prefLabel "has Funder Info"@en .

gdmt:hasDatasetIdentifierSubType a owl:ObjectProperty;
  rdfs:label "has Dataset Identifier Sub Type"@en;
  skos:definition "The subtype of the identifier used to specify the dataset being described."@en;
  skos:prefLabel "has Dataset Identifier Sub Type"@en .

gdmt:hasRelatedResourceIdentifierSubType a owl:ObjectProperty;
  rdfs:label "has Related Resource Identifier Sub Type"@en;
  skos:definition "The subtype of the identifier used to specify the related resource."@en;
  skos:prefLabel "has Related Resource Identifier Sub Type"@en .

gdmt:hasPrimaryLanguage a owl:ObjectProperty;
  rdfs:label "has Primary Language"@en;
  skos:definition "Primary language used to present the dataset (if multiple languages are present). Use a code from IETF BCP 47"@en;
  skos:prefLabel "has Primary Language"@en .

gdmt:hasSubjectIRI a owl:DatatypeProperty;
  rdfs:label "has Subject IRI"@en;
  skos:definition "The IRI of a concept (keyword, classification code, or controlled key phrase) that defines the dataset or indicates for which concepts the dataset can be used."@en;
  skos:prefLabel "has Subject IRI"@en .

gdmt:hasSubjectScheme a owl:DatatypeProperty;
  rdfs:label "has Subject Scheme"@en;
  skos:definition "The name of the scheme or authority used for the Subject Concept IRI."@en;
  skos:prefLabel "has Subject Scheme"@en .

gdmt:hasSubjectSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has Subject Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Subject Concept IRI."@en;
  skos:prefLabel "has Subject Scheme IRI"@en .

gdmt:hasKeyword a owl:DatatypeProperty;
  rdfs:label "has Keyword"@en;
  skos:definition "Free text subject, keyword, classification code, or key phrase describing the dataset or purpose for which the dataset can be used."@en;
  skos:prefLabel "has Keyword"@en .

gdmt:hasCreatorIdentifier a owl:DatatypeProperty;
  rdfs:label "has Creator Identifier"@en;
  skos:definition "Globally unique string that identifies the creator (an individual or legal entity)."@en;
  skos:prefLabel "has Creator Identifier"@en .

gdmt:hasCreatorIdentifierScheme a owl:DatatypeProperty;
  rdfs:label "has Creator Identifier Scheme"@en;
  skos:definition "The name of the scheme or authority used for the Creator Identifier."@en;
  skos:prefLabel "has Creator Identifier Scheme"@en .

gdmt:hasCreatorIdentifierSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has Creator Identifier Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Creator Identifier."@en;
  skos:prefLabel "has Creator Identifier Scheme IRI"@en .

gdmt:hasCreatorAffiliation a owl:ObjectProperty;
  rdfs:label "has Creator Affiliation"@en;
  skos:definition "The organizational or institutional affiliation of the creator."@en;
  skos:prefLabel "has Creator Affiliation"@en .

gdmt:hasCreatorAffiliationIdentifier a owl:DatatypeProperty;
  rdfs:label "has Creator Affiliation Identifier"@en;
  skos:definition "Globally unique string that identifies the organizational affiliation of the creator."@en;
  skos:prefLabel "has Creator Affiliation Identifier"@en .

gdmt:hasCreatorAffiliationIdentifierScheme a owl:DatatypeProperty;
  rdfs:label "has Creator Affiliation Identifier Scheme"@en;
  skos:definition "The name of the scheme or authority used for the Creator Affiliation Identifier."@en;
  skos:prefLabel "has Creator Affiliation Identifier Scheme"@en .

gdmt:hasCreatorAffiliationIdentifierSchemeIRI a owl:ObjectProperty;
  rdfs:label "has Creator Affiliation Identifier Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Creator Affiliation Identifier."@en;
  skos:prefLabel "has Creator Affiliation Identifier Scheme IRI"@en .

gdmt:hasCreatorRole a owl:ObjectProperty;
  rdfs:label "has Creator Role"@en;
  skos:definition "The role of the creator in bringing the described dataset into existence."@en;
  skos:prefLabel "has Creator Role"@en .

gdmt:hasCreatorType a owl:ObjectProperty;
  rdfs:label "has Creator Type"@en;
  skos:definition "The type of the creator of the described dataset (organization or person)."@en;
  skos:prefLabel "has Creator Type"@en .

gdmt:hasContributorIdentifier a owl:DatatypeProperty;
  rdfs:label "has Contributor Identifier"@en;
  skos:definition "Globally unique string that identifies the contributor (an individual or legal entity)."@en;
  skos:prefLabel "has Contributor Identifier"@en .

gdmt:hasContributorIdentifierScheme a owl:DatatypeProperty;
  rdfs:label "has Contributor Identifier Scheme"@en;
  skos:definition "The name of the scheme or authority used for the Contributor Identifier."@en;
  skos:prefLabel "has Contributor Identifier Scheme"@en .

gdmt:hasContributorIdentifierSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has Contributor Identifier Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Contributor Identifier."@en;
  skos:prefLabel "has Contributor Identifier Scheme IRI"@en .

gdmt:hasContributorAffiliation a owl:ObjectProperty;
  rdfs:label "has Contributor Affiliation"@en;
  skos:definition "The organizational or institutional affiliation of the contributor."@en;
  skos:prefLabel "has Contributor Affiliation"@en .

gdmt:hasContributorAffiliationIdentifier a owl:DatatypeProperty;
  rdfs:label "has Contributor Affiliation Identifier"@en;
  skos:definition "Globally unique string that identifies the organizational affiliation of the contributor."@en;
  skos:prefLabel "has Contributor Affiliation Identifier"@en .

gdmt:hasContributorAffiliationIdentifierScheme a owl:DatatypeProperty;
  rdfs:label "has Contributor Affiliation Identifier Scheme"@en;
  skos:definition "The name of the scheme or authority used for the Contributor Affiliation Identifier."@en;
  skos:prefLabel "has Contributor Affiliation Identifier Scheme"@en .

gdmt:hasContributorAffiliationIdentifierSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has Contributor Affiliation Identifier Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Contributor Affiliation Identifier."@en;
  skos:prefLabel "has Contributor Affiliation Identifier Scheme IRI"@en .

gdmt:hasContributorRole a owl:ObjectProperty;
  rdfs:label "has Contributor Role"@en;
  skos:definition "The role of the contributor in bringing the described dataset into existence."@en;
  skos:prefLabel "has Contributor Role"@en .

gdmt:hasContributorType a owl:ObjectProperty;
  rdfs:label "has Contributor Type"@en;
  skos:definition "The type of the contributor of the described dataset (organization or person)."@en;
  skos:prefLabel "has Contributor Type"@en .

gdmt:hasPublisherIdentifier a owl:DatatypeProperty;
  rdfs:label "has Publisher Identifier"@en;
  skos:definition "Globally unique string that identifies the publisher."@en;
  skos:prefLabel "has Publisher Identifier"@en .

gdmt:hasPublisherIdentifierScheme a owl:DatatypeProperty;
  rdfs:label "has Publisher Identifier Scheme"@en;
  skos:definition "The name of the scheme or authority used for the Publisher Identifier."@en;
  skos:prefLabel "has Publisher Identifier Scheme"@en .

gdmt:hasPublisherIdentifierSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has Publisher Identifier Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Publisher Identifier."@en;
  skos:prefLabel "has Publisher Identifier Scheme IRI"@en .

gdmt:hasPublisherAddress a owl:DatatypeProperty;
  rdfs:label "has Publisher Address"@en;
  skos:definition "An address of the publisher."@en;
  skos:prefLabel "has Publisher Address"@en .

gdmt:hasLicenseName a owl:DatatypeProperty;
  rdfs:label "has License Name"@en;
  skos:definition "A standardized version of the license name."@en;
  skos:prefLabel "has License Name"@en .

gdmt:hasLicenseIdentifier a owl:DatatypeProperty;
  rdfs:label "has License Identifier"@en;
  skos:definition "IRI of the license for the dataset being described."@en;
