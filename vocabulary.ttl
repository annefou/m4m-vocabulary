gdmt:hasDistributorType a owl:ObjectProperty;
  rdfs:label "has Distributor Type"@en;
  skos:definition "The type of the distributor of the described dataset (organization, or person)."@en;
  skos:prefLabel "has Distributor Type"@en .

gdmt:hasDataStream a owl:DatatypeProperty;
  rdfs:label "has Data Stream"@en;
  skos:definition "Human readable name of collection of records coming from a data source."@en;
  skos:prefLabel "has Data Stream"@en .

gdmt:hasDataStreamIRI a owl:DatatypeProperty;
  rdfs:label "has Data Stream IRI"@en;
  skos:definition "Unique identifier of collection of records coming from a data source."@en;
  skos:prefLabel "has Data Stream IRI"@en .

gdmt:hasDataStreamScheme a owl:DatatypeProperty;
  rdfs:label "has Data Stream Scheme"@en;
  skos:definition "The name of the scheme or authority used for the Data Stream Identifier."@en;
  skos:prefLabel "has Data Stream Scheme"@en .

gdmt:hasDataStreamSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has Data Stream Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Data Stream Identifier."@en;
  skos:prefLabel "has Data Stream Scheme IRI"@en .

gdmt:hasDataSource a owl:DatatypeProperty;
  rdfs:label "has Data Source"@en;
  skos:definition "Human readable name of data source from which the variable(s) come(s)."@en;
  skos:prefLabel "has Data Source"@en .

gdmt:hasDataSourceIRI a owl:DatatypeProperty;
  rdfs:label "has Data Source IRI"@en;
  skos:definition "IRI describing the data source (e.g., PID of an instrument)."@en;
  skos:prefLabel "has Data Source IRI"@en .

gdmt:hasDataSourceScheme a owl:DatatypeProperty;
  rdfs:label "has Data Source Scheme"@en;
  skos:definition "The name of the scheme or authority used for the Data Source Identifier."@en;
  skos:prefLabel "has Data Source Scheme"@en .

gdmt:hasDataSourceSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has Data Source Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Data Source Identifier."@en;
  skos:prefLabel "has Data Source Scheme IRI"@en .

gdmt:hasVariable a owl:DatatypeProperty;
  rdfs:label "has Variable"@en;
  skos:definition "Human readable name of variable being recorded in datastream."@en;
  skos:prefLabel "has Variable"@en .

gdmt:hasVariableIRI a owl:DatatypeProperty;
  rdfs:label "has Variable IRI"@en;
  skos:definition "IRI describing the variable."@en;
  skos:prefLabel "has Variable IRI"@en .

gdmt:hasVariableScheme a owl:DatatypeProperty;
  rdfs:label "has Variable Scheme"@en;
  skos:definition "The name of the scheme or authority used for the Variable Identifier."@en;
  skos:prefLabel "has Variable Scheme"@en .

gdmt:hasVariableSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has Variable Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Variable Identifier."@en;
  skos:prefLabel "has Variable Scheme IRI"@en .

gdmt:hasPointLatitude a owl:DatatypeProperty;
  rdfs:label "has Point Latitude"@en;
  skos:definition "Geographic latitude of a point which represents a location of dataset collection."@en;
  skos:prefLabel "has Point Latitude"@en .

gdmt:hasPointLongitude a owl:DatatypeProperty;
  rdfs:label "has Point Longitude"@en;
  skos:definition "Geographic longitude of a point which represents a location of dataset collection."@en;
  skos:prefLabel "has Point Longitude"@en .

gdmt:hasWestBoundLongitude a owl:DatatypeProperty;
  rdfs:label "has West Bound Longitude"@en;
  skos:definition "The most west longitude of a bounding box representing an area of dataset collection."@en;
  skos:prefLabel "has West Bound Longitude"@en .

gdmt:hasEastBoundLongitude a owl:DatatypeProperty;
  rdfs:label "has East Bound Longitude"@en;
  skos:definition "The most east longitude of a bounding box representing an area of dataset collection."@en;
  skos:prefLabel "has East Bound Longitude"@en .

gdmt:hasSouthBoundLatitude a owl:DatatypeProperty;
  rdfs:label "has South Bound Latitude"@en;
  skos:definition "The most south latitude of a bounding box representing an area of dataset collection."@en;
  skos:prefLabel "has South Bound Latitude"@en .

gdmt:hasNorthBoundLatitude a owl:DatatypeProperty;
  rdfs:label "has North Bound Latitude"@en;
  skos:definition "The most north latitude of a bounding box representing an area of dataset collection."@en;
  skos:prefLabel "has North Bound Latitude"@en .

gdmt:hasPolygonPointLatitude a owl:DatatypeProperty;
  rdfs:label "has Polygon Point Latitude"@en;
  skos:definition "Geographical latitude of a polygon point which represents one vertex of an area of dataset collection."@en;
  skos:prefLabel "has Polygon Point Latitude"@en .

gdmt:hasPolygonPointLongitude a owl:DatatypeProperty;
  rdfs:label "has Polygon Point Longitude"@en;
  skos:definition "Geographical longitude of a polygon point which represents one vertex of an area of dataset collection."@en;
  skos:prefLabel "has Polygon Point Longitude"@en .

gdmt:hasGeoLocationDescription a owl:DatatypeProperty;
  rdfs:label "has Geo Location Description"@en;
  skos:definition "The description of a geographical location of dataset collection."@en;
  skos:prefLabel "has Geo Location Description"@en .

gdmt:hasVerticalResolutionInMeters a owl:DatatypeProperty;
  rdfs:label "has Vertical Resolution In Meters"@en;
  skos:definition "Minimum vertical separation resolvable in a dataset, measured in meters."@en;
  skos:prefLabel "has Vertical Resolution In Meters"@en .

gdmt:hasVerticalExtentMaximumValue a owl:DatatypeProperty;
  rdfs:label "has Vertical Extent Maximum Value"@en;
  skos:definition "A maximum vertical extent of dataset collection. In case of depth, this represents a minimum depth of dataset collection."@en;
  skos:prefLabel "has Vertical Extent Maximum Value"@en .

gdmt:hasVerticalExtentMinimumValue a owl:DatatypeProperty;
  rdfs:label "has Vertical Extent Minimum Value"@en;
  skos:definition "A minimum vertical extent of dataset collection. In case of depth, this represents a maximum depth of dataset collection."@en;
  skos:prefLabel "has Vertical Extent Minimum Value"@en .

gdmt:hasVerticalExtentUnit a owl:ObjectProperty;
  rdfs:label "has Vertical Extent Unit"@en;
  skos:definition "Unit used to specify Vertical Extent."@en;
  skos:prefLabel "has Vertical Extent Unit"@en .

gdmt:hasVerticalExtentUnitIRI a owl:DatatypeProperty;
  rdfs:label "has Vertical Extent Unit IRI"@en;
  skos:definition "The IRI for the unit used to specify Vertical Extent."@en;
  skos:prefLabel "has Vertical Extent Unit IRI"@en .

gdmt:hasVerticalExtentUnitScheme a owl:DatatypeProperty;
  rdfs:label "has Vertical Extent Unit Scheme"@en;
  skos:definition "The name of the scheme or authority used for the Vertical Extent Unit Identifier."@en;
  skos:prefLabel "has Vertical Extent Unit Scheme"@en .

gdmt:hasVerticalExtentUnitSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has Vertical Extent Unit Scheme IRI"@en;
  skos:definition "The IRI of the scheme or authority used for the Vertical Extent Unit Identifier."@en;
  skos:prefLabel "has Vertical Extent Unit Scheme IRI"@en .

gdmt:hasVerticalExtentDatum a owl:DatatypeProperty;
  rdfs:label "has Vertical Extent Datum"@en;
  skos:definition "The name of the Datum (reference frame) used for the Vertical Extent values."@en;
  skos:prefLabel "has Vertical Extent Datum"@en .

gdmt:hasVerticalExtentDatumIRI a owl:DatatypeProperty;
  rdfs:label "has Vertical Extent Datum IRI"@en;
  skos:definition "The unique identifier of the Datum (reference frame) used for the Vertical Extent values."@en;
  skos:prefLabel "has Vertical Extent Datum IRI"@en .

gdmt:hasTemporalExtentMinimumValue a owl:DatatypeProperty;
  rdfs:label "has Temporal Extent Minimum Value"@en;
  skos:definition "The start date of the data."@en;
  skos:prefLabel "has Temporal Extent Minimum Value"@en .

gdmt:hasTemporalExtentMaximumValue a owl:DatatypeProperty;
  rdfs:label "has Temporal Extent Maximum Value"@en;
  skos:definition "The end date of the data."@en;
  skos:prefLabel "has Temporal Extent Maximum Value"@en .

gdmt:hasTemporalResolution a owl:DatatypeProperty;
  rdfs:label "has Temporal Resolution"@en;
  skos:definition "Interval between two consecutive data records."@en;
  skos:prefLabel "has Temporal Resolution"@en .

gdmt:hasDuration a owl:DatatypeProperty;
  rdfs:label "has Duration"@en;
  skos:definition "The total duration of data temporal coverage."@en;
  skos:prefLabel "has Duration"@en .

gdmt:hasAwardTitle a owl:DatatypeProperty;
  rdfs:label "has Award Title"@en;
  skos:definition "The human readable title of the award, grant, or other specific funding mechanism for the described dataset."@en;
  skos:prefLabel "has Award Title"@en .

gdmt:hasAwardPageIRI a owl:DatatypeProperty;
  rdfs:label "has Award Page IRI"@en;
  skos:definition "The IRI leading to a page provided by the funder for more information about the award (grant)."@en;
  skos:prefLabel "has Award Page IRI"@en .

gdmt:hasAwardLocalIdentifier a owl:DatatypeProperty;
  rdfs:label "has Award Local Identifier"@en;
  skos:definition "The code assigned by the funding provider to a sponsored award (grant). (Can be an IRI, but typically is not.)"@en;
  skos:prefLabel "has Award Local Identifier"@en .

gdmt:hasFunderName a owl:DatatypeProperty;
  rdfs:label "has Funder Name"@en;
  skos:definition "Name of the organization providing the funding for the described dataset ('funding provider')."@en;
  skos:prefLabel "has Funder Name"@en .

gdmt:hasFunderIdentifier a owl:DatatypeProperty;
  rdfs:label "has Funder Identifier"@en;
  skos:definition "Globally unique string that identifies the funding provider."@en;
  skos:prefLabel "has Funder Identifier"@en .

gdmt:hasFunderIdentifierScheme a owl:DatatypeProperty;
  rdfs:label "has Funder Identifier Scheme"@en;
  skos:definition "Name of the schema for the funderIdentifier."@en;
  skos:prefLabel "has Funder Identifier Scheme"@en .

gdmt:hasFunderIdentifierSchemeIRI a owl:DatatypeProperty;
  rdfs:label "has Funder Identifier Scheme IRI"@en;
  skos:definition "IRI of the schema for the funder identifier."@en;
  skos:prefLabel "has Funder Identifier Scheme IRI"@en .

gdmt:ResourceTypeCategory a skos:Concept;
  skos:definition "This is a set of controlled terms from DataCite/DCMI for the category of resource being described."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:narrower gdmt:Audiovisual, gdmt:Collection, gdmt:DataCatalog, gdmt:DataPaper,
    gdmt:DataStream, gdmt:Dataset, gdmt:Event, gdmt:Image, gdmt:InteractiveResource, gdmt:Model,
    gdmt:PhysicalObject, gdmt:Service, gdmt:Software, gdmt:Sound;
  skos:prefLabel "Resource Type Category"@en;
  skos:topConceptOf <http://purl.org/m4m> .

gdmt:Audiovisual a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "A series of visual representations imparting an impression of motion when shown in succession. May or may not include sound."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Audiovisual"@en .

gdmt:Collection a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "An aggregation of resources, which may encompass collections of one resourceType as well as those of mixed types. A collection is described as a group; its parts may also be separately described."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Collection"@en .

gdmt:DataPaper a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "A factual and objective publication with a focused intent to identify and describe specific data, sets of data, or data collections to facilitate discoverability."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Data Paper"@en .

gdmt:DataCatalog a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "A collection of datasets."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Data Catalog"@en .

gdmt:Dataset a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "Dataset is a logical grouping of one or more data streams. It is can be provided in various data distributions. Dataset might be part of (parent) data catalog."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Dataset"@en .

gdmt:DataStream a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "Data stream represent a collection of one or more variable records originating from a single data source (e.g., sensor) which properties remain unchanged during the data stream existence. In some use-cases, the data source location is required to remain unchanged (e.g., sensors attached to a meteorological tower). Data stream can grow over time."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Data Stream"@en .

gdmt:Event a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "A non-persistent, time-based occurrence."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Event"@en .

gdmt:Image a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "A visual representation other than text"@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Image"@en .

gdmt:InteractiveResource a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "A resource requiring interaction from the user to be understood, executed, or experienced"@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Interactive Resource"@en .

gdmt:Model a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "An abstract, conceptual, graphical, mathematical or visualization model that represents empirical objects, phenomena, or physical processes."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Model"@en .

gdmt:PhysicalObject a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "An inanimate, three-dimensional object or substance."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Physical Object"@en .

gdmt:Service a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "An organized system of apparatus, appliances, staff, etc., for supplying some function(s) required by end users."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Service"@en .

gdmt:Software a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "A computer program in source code (text) or compiled form. Use this type for all software components supporting scholarly research."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Software"@en .

gdmt:Sound a skos:Concept;
  skos:broader gdmt:ResourceTypeCategory;
  skos:definition "A resource primarily intended to be heard."@en;
  skos:inScheme <http://purl.org/m4m>;
  skos:prefLabel "Sound"@en .
